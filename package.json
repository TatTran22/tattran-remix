{
  "name": "tattran-remix",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "deploy": "pnpm run build && wrangler pages deploy ./build/client",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev ./build/client",
    "typecheck": "tsc",
    "typegen": "wrangler types",
    "preview": "pnpm run build && wrangler pages dev",
    "drizzle-kit": "drizzle-kit",
    "drizzle:generate": "drizzle-kit generate",
    "drizzle:up": "drizzle-kit up",
    "db:migrate:local": "pnpm run drizzle:generate && pnpm dlx wrangler d1 migrations apply tattran-remix --local",
    "db:migrate": "pnpm run drizzle:generate && pnpm dlx wrangler d1 migrations apply tattran-remix",
    "d1:execute:local": "pnpm dlx wrangler d1 execute tattran-remix --local --file=./app/drizzle/migrations/0000_petite_freak.sql",
    "d1:execute": "pnpm dlx wrangler d1 execute tattran-remix --remote --file=./app/drizzle/migrations/0000_petite_freak.sql",
    "wrangler:dev": "wrangler dev"
  },
  "dependencies": {
    "@iconify/react": "^4.1.1",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@remix-run/cloudflare": "^2.9.2",
    "@remix-run/cloudflare-pages": "^2.9.2",
    "@remix-run/react": "^2.9.2",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.30.10",
    "isbot": "^4.4.0",
    "isomorphic-base64": "^1.0.2",
    "lucide-react": "^0.379.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-switch": "^7.0.0",
    "react-type-animation": "^3.2.0",
    "remix-themes": "^1.3.1",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "tiny-invariant": "^1.3.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240512.0",
    "@remix-run/dev": "^2.9.2",
    "@tailwindcss/typography": "^0.5.13",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.19",
    "drizzle-kit": "^0.21.4",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-tsconfig-paths": "^4.3.2",
    "wrangler": "3.57.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}